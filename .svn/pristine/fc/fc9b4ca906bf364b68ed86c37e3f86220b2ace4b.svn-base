page-videos {
    .ion-list {
        display: flex;
        flex-wrap: wrap;
        justify-content: space-between;
        .ion-item {
            border: none !important;
            box-shadow: 0 3px 6px rgba(0, 0, 0, 0.16), 0 3px 6px rgba(0, 0, 0, 0.23);
            margin-bottom: 16px;
            padding-left: 0;
        }
    }
    .item-md [item-left],
    .item-md [item-right],
    .item-md [item-start],
    .item-md [item-end] {
        margin: 0;
    }
}